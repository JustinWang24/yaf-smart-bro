{
  "name": "justinwang/yar-start-up-eloquent",
  "description": "使用 Yaf 框架开发 API 服务的起始模板",
  "type": "project",
  "require-dev": {
    "fzaninotto/faker": "^1.9.0",
    "guzzlehttp/guzzle": "^6.5.0",
    "phpunit/phpunit": "^7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Justin Wang",
      "email": "justin@webmelbourne.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "doctrine/dbal": "^2.10",
    "illuminate/database": "^7.14.0"
  },
  "autoload": {
    "psr-4": {
      "AppServices\\": "application/services/",
      "AppModels\\": "application/models/"
    },
    "files": ["application/helpers/common.php"]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
