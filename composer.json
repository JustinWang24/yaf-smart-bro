{
  "name": "justinwang/yar-start-up-eloquent",
  "description": "使用 Yaf 框架开发 API 服务的起始模板",
  "type": "project",
  "require-dev": {
    "fzaninotto/faker": "^1.8",
    "phpunit/phpunit": "^7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Justin Wang",
      "email": "justinwang24@yahoo.com.au"
    }
  ],
  "require": {
    "illuminate/database": "^5.8",
    "nesbot/carbon": "^2.23",
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "AppServices\\": "application/services/",
      "AppModels\\": "application/models/"
    },
    "files": ["application/helpers/common.php"]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
